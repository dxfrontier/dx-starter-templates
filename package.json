{
  "name": "@dxfrontier/projen-template-projects",
  "repository": {
    "type": "git",
    "url": "https://github.com/dxfrontier/projen-template-projects.git"
  },
  "scripts": {
    "build": "npx projen build",
    "clobber": "npx projen clobber",
    "commit": "npx projen commit",
    "compat": "npx projen compat",
    "compile": "npx projen compile",
    "default": "npx projen default",
    "docgen": "npx projen docgen",
    "eject": "npx projen eject",
    "eslint": "npx projen eslint",
    "format:fix": "npx projen format:fix",
    "format:message": "npx projen format:message",
    "install-dependencies": "npx projen install-dependencies",
    "package": "npx projen package",
    "package-all": "npx projen package-all",
    "package:js": "npx projen package:js",
    "post-compile": "npx projen post-compile",
    "pre-compile": "npx projen pre-compile",
    "prepare": "npx projen prepare",
    "test": "npx projen test",
    "test:watch": "npx projen test:watch",
    "watch": "npx projen watch",
    "projen": "npx projen"
  },
  "author": {
    "name": "Mathias von Kaiz",
    "email": "mathias.von-kaiz@abs-gmbh.de",
    "organization": false
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/prompt-cli": "^19.5.0",
    "@commitlint/types": "^19.5.0",
    "@types/jest": "^29",
    "@types/node": "^22",
    "@typescript-eslint/eslint-plugin": "^8",
    "@typescript-eslint/parser": "^8",
    "constructs": "^10.0.0",
    "eslint": "^9",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "jest": "^29",
    "jest-junit": "^15",
    "jsii": "~5.5.0",
    "jsii-diff": "^1.104.0",
    "jsii-docgen": "^10.5.0",
    "jsii-pacmak": "^1.104.0",
    "jsii-rosetta": "~5.5.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "projen": "0.90.3",
    "ts-jest": "^29",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "projen": "^0.90.3"
  },
  "dependencies": {
    "construct": "^1.0.0"
  },
  "bundledDependencies": [
    "construct"
  ],
  "main": "lib/index.js",
  "license": "Apache-2.0",
  "version": "0.0.0",
  "jest": {
    "coverageProvider": "v8",
    "testMatch": [
      "<rootDir>/@(src|test)/**/*(*.)@(spec|test).ts?(x)",
      "<rootDir>/@(src|test)/**/__tests__/**/*.ts?(x)",
      "<rootDir>/@(projenrc)/**/*(*.)@(spec|test).ts?(x)",
      "<rootDir>/@(projenrc)/**/__tests__/**/*.ts?(x)"
    ],
    "clearMocks": true,
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "clover",
      "cobertura",
      "text"
    ],
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "watchPathIgnorePatterns": [
      "/node_modules/"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test-reports"
        }
      ]
    ],
    "transform": {
      "^.+\\.[t]sx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.dev.json"
        }
      ]
    }
  },
  "types": "lib/index.d.ts",
  "stability": "stable",
  "jsii": {
    "outdir": "dist",
    "targets": {},
    "tsc": {
      "outDir": "lib",
      "rootDir": "src"
    }
  },
  "files": [
    "lib",
    "README.md",
    "LICENSE",
    ".jsii"
  ],
  "lint-staged": {
    "**/*.ts": [
      "npm run eslint",
      "npm run format:fix"
    ]
  },
  "//": "~~ Generated by projen. To modify, edit .projenrc.ts and run \"npx projen\"."
}
